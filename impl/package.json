{
  "name": "xyna",
  "title": "Xyna Modeller",
  "version": "10.1.0.2",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development --ssl=true --ssl-key=./ssl/server-public.pem",
    "build": "ng build --configuration=development",
    "release": "ng build xyna --configuration=production --extract-licenses",
    "lint": "eslint --color -c .eslintrc.json --ext .ts ./projects/xyna/src/app",
    "lint:fix": "npm run lint -- --fix",
    "tsc": "tsc",
    "extract-i18n": "ng xi18n xyna --i18n-format xlf --output-path ./locale --i18n-locale en-US && ng run xyna:xliffmerge"
  },
  "dependencies": {
    "@angular/animations": "17.3.3",
    "@angular/cdk": "17.3.3",
    "@angular/common": "17.3.3",
    "@angular/compiler": "17.3.3",
    "@angular/core": "17.3.3",
    "@angular/forms": "17.3.3",
    "@angular/localize": "17.3.3",
    "@angular/material": "17.3.3",
    "@angular/platform-browser": "17.3.3",
    "@angular/platform-browser-dynamic": "17.3.3",
    "@angular/router": "17.3.3",
    "@angular/service-worker": "17.3.3",
    "escape-string-regexp": "5.0.0",
    "hammerjs": "2.0.8",
    "monaco-editor": "0.44.0",
    "ngx-monaco-editor-v2": "17.0.1",
    "rxjs": "7.5.6",
    "three": "0.129.0",
    "tslib": "2.4.0",
    "vite": "^4.5.2",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.2",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular/cli": "17.3.3",
    "@angular/compiler-cli": "17.3.3",
    "@angular/language-service": "17.3.3",
    "@types/node": "18.0.6",
    "@types/offscreencanvas": "2019.7.0",
    "@types/resize-observer-browser": "0.1.7",
    "@types/three": "0.129.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.51.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-zeta": "file:projects/xyna/src/app/zeta/lint/plugins/eslint-plugin-zeta",
    "ts-node": "10.9.1",
    "typescript": "5.3.3"
  }
}
